{
  "name": "diogel",
  "version": "0.0.5",
  "description": "Privacy focused nostr extension",
  "productName": "Diogel",
  "author": "threenine.io <support@threenine.co.uk>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "vitest",
    "test:run": "vitest run",
    "dev": "quasar dev",
    "build": "quasar build",
    "postinstall": "quasar prepare",
    "build:chrome": "quasar build -m bex -T chrome",
    "release": "npm run build && npm run build:chrome && changelogen --release && git push --follow-tags"
  },
  "dependencies": {
    "@noble/ciphers": "^0.5.3",
    "@noble/curves": "^1.9.7",
    "@noble/hashes": "^1.8.0",
    "@quasar/extras": "^1.16.4",
    "@scure/base": "^1.2.6",
    "@scure/bip32": "^1.7.0",
    "@scure/bip39": "^1.6.0",
    "@zip.js/zip.js": "^2.8.11",
    "archiver-zip-encrypted": "^2.0.0",
    "axios": "^1.2.1",
    "dexie": "^4.2.1",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "nostr-tools": "^2.19.4",
    "pinia": "^3.0.1",
    "quasar": "^2.16.0",
    "vue": "^3.5.22",
    "vue-i18n": "^11.0.0",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@quasar/app-vite": "^2.1.0",
    "@types/crypto-js": "^4.2.2",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.5.9",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.2",
    "changelogen": "^0.6.2",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.4.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.3.3",
    "typescript": "^5.9.2",
    "vite-plugin-checker": "^0.11.0",
    "vitest": "^4.0.15",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.7"
  },
  "pnpm": {
    "neverBuiltDependencies": []
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
